    <section class="container-fluid" style="background-color:  #fff ;padding: 80px 20px;">
        <div class="container">
            <div class="row">
                <div class="">
                    <h2 data-aos="fade-right" data-aos-duration="800" style="color: #222222;
                          text-align: left;
                          font-family: 'Raleway', sans-serif;
                          padding-bottom: 70px;
                          font-size: 45px;">{{ post.postTitle|raw }}</h2>
                </div>
            </div>
                    <div class="row" id="blog" style="text-align: left;">
                        <div style="font-weight: bold">
                            {{ post.postContent|raw }}
{#                            <a href="{{ path("blog_contenu",{id:p.id}) }}">Lire la contenue</a>#}
                        </div>
                    </div>
            {{ post.commentaires|length }} commentaires
            <p>------------------------------------------</p><br>
            <div class="comment-list">
                {% if post.commentaires|length > 0 %}
                    {% for c in post.commentaires %}
                        {% set athor=(c.author==null)?''~c.authorClient.responabilite~' de '~c.authorClient.client.nom:''~c.author.nom %}
                        <div style="font-weight: bold; color: #0c83e2">{{ athor~' :' }}</div>
                        <div class="comment">{{ c.contenu }}</div><hr style="">
                    {% endfor %}
                {% endif %}
            </div>
            {% if app.user %}
            <div class="form-comment">
                {{ form_start(comment) }}
                {{ form_row(comment.contenu) }}
                <input type="submit" class="btn-sm btn-primary" value="Commenter">
                {{ form_end(comment) }}
            </div>
            {% endif %}
        </div>
    </section>
{% endblock %}